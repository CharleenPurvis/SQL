Create a Candle store database



CREATE TABLE Candles (id INTEGER PRIMARY KEY, Candles TEXT, Size INTEGER, Price INTEGER, Color TEXT, Quantity INTEGER);

INSERT into Candles VALUES (1, "Vanilla", 12, 16.50, "White", 5); 
INSERT into Candles VALUES (2, "Cinnamon", 12, 15, "Red", 7);
INSERT into Candles VALUES (3, "Strawberry", 12, 17.50, "Red", 4); 
INSERT into Candles VALUES (4, "Tobacco", 12, 16.50, "Brown", 6);
INSERT into Candles VALUES (5, "Pumpkin", 12, 19.0, "Orange", 3);

SELECT * FROM Candles;

-Displayed candles in order by price

SELECT * from candles order by price; 
